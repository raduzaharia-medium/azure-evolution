{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Node Functions",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "preLaunchTask": "func: host start"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Web Client",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/web-client/server.mjs"
    }
  ]
}
